@model RoadRecordViolationSystem.ViewModels.AccidentSceneInvolveViewModel
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_NonEnforcerLayout.cshtml";
}

<div class="site-content">
    <nav aria-labelledby="breadcrumb">
        <ol class="breadcrumb px-4">
            <li class="breadcrumb-item "><a class="text-decoration-none" asp-controller="Dashboard" asp-action="GetDashboardView">Dashboard</a></li>
            <li class="breadcrumb-item active">Accidents</li>
        </ol>
    </nav>
    <div class="row">
        <div class="col-md-12">
            <div class="table-responsive-sm">
                <partial name="_AccidentTablePartial.cshtml" />
            </div>
        </div>
    </div>
    <div id="view-more-modal-holder"></div>
</div>
@section scripts{
    <partial name="/Views/Shared/_ValidationScriptsPartial.cshtml" />
    <script type="text/javascript" src="~/js/Scoped/Accident/accident.js"></script>
    <script type="text/javascript" src="~/js/Global/ajaxRequest.js"></script>
}
