@model RoadRecordViolationSystem.Models.ApplicationUser
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@


<div class="modal-body">
    <p>Do you want to Delete User <bold>@Model.UserName</bold> ?</p>
</div>

<div class="modal-footer">
   <span class="float-end"><button class="btn btn-danger mx-2" onclick="requestAjaxDelete('@Url.Action("Delete","AdminAccounts")', {id:@Model.UsersInfoId ,userName:'@Model.UserName'})">Delete</button><button class="btn btn-outline-dark" data-bs-dismiss="modal">Cancel</button></span>
</div>

@Html.AntiForgeryToken();

