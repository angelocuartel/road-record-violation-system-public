@model RoadRecordViolationSystem.Models.ContestSchedule
@*
    For more information on enabling MVC for empty projects, visit http://go.microsoft.com/fwlink/?LinkID=397860
*@
<div class="modal fade" id="proof-modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header fw-bold">
                Proof Video
            </div>
            <div class="modal-body">
                
                <video id="video-holder" width="500" height="500" class="mx-auto d-block" controls autoplay>
                <source src="@Url.Content(ViewBag.VideoPath)" type="video/mp4" />
                </video>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-outline-dark">Close</button>
            </div>
        </div>
    </div>
</div>